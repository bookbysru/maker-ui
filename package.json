{
  "private": true,
  "scripts": {
    "prepare": "lerna run prepare",
    "clean": "lerna run clean",
    "test": "jest"
  },
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/react": "^9.4.0",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.27",
    "lerna": "^3.20.2",
    "lint-staged": "^9.5.0",
    "microbundle": "^0.11.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0"
  },
  "jest": {
    "testMatch": [
      "**/packages/**/test/*.js"
    ],
    "snapshotSerializers": [
      "jest-emotion"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {}
